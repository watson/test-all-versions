{
  "name": "test-all-versions",
  "version": "6.1.0",
  "description": "Run your test suite against all published versions of a dependency",
  "bin": {
    "tav": "index.js"
  },
  "dependencies": {
    "after-all-results": "^2.0.0",
    "ansi-diff-stream": "^1.2.1",
    "cli-spinners": "^2.9.2",
    "deepmerge": "^4.3.1",
    "import-fresh": "^3.3.0",
    "is-ci": "^3.0.1",
    "js-yaml": "^4.1.0",
    "log-symbols": "^4.1.0",
